CREATE TABLE USUARIO (
    ID NUMBER NOT NULL,
    NOME VARCHAR2(100) NOT NULL,
    CPF NUMBER(11) NOT NULL,
    PRIMARY KEY (ID)
);

CREATE TABLE CRIADOR (
    ID_USUARIO NUMBER NOT NULL,
    PRIMARY KEY (ID_USUARIO),
    FOREIGN KEY (ID_USUARIO) REFERENCES USUARIO(ID)
);

CREATE TABLE PARTICIPANTE (
    ID_USUARIO NUMBER NOT NULL,
    PRIMARY KEY (ID_USUARIO),
    FOREIGN KEY (ID_USUARIO) REFERENCES USUARIO(ID)
);

CREATE TABLE EVENTO (
    ID NUMBER NOT NULL,
    NOME VARCHAR2(100) NOT NULL,
    DATA_EVENTO DATE NOT NULL,
    LOCAL VARCHAR2(100) NOT NULL,
    DESCRICAO VARCHAR2(500),
    ID_CRIADOR NUMBER NOT NULL,
    PRIMARY KEY (ID),
    FOREIGN KEY (ID_CRIADOR) REFERENCES CRIADOR(ID_USUARIO)
);

CREATE TABLE SUBEVENTO (
    ID NUMBER NOT NULL,
    NOME VARCHAR2(100) NOT NULL,
    DESCRICAO VARCHAR2(500),
    LOCAL VARCHAR2(100) NOT NULL,
    DATA_EVENTO DATE NOT NULL,
    ID_EVENTO NUMBER NOT NULL,
    PRIMARY KEY (ID),
    FOREIGN KEY (ID_EVENTO) REFERENCES EVENTO(ID)
);

CREATE TABLE SESSAO (
    ID NUMBER NOT NULL,
    HORA_TERMINO TIMESTAMP NOT NULL,
    HORA_INICIO TIMESTAMP NOT NULL,
    DESCRICAO VARCHAR2(500),
    DATA_EVENTO DATE NOT NULL,
    NOME VARCHAR2(100) NOT NULL,
    PRIMARY KEY (ID)
);

CREATE TABLE SESSAO_EVENTO (
    ID_SESSAO NUMBER NOT NULL,
    ID_EVENTO NUMBER NOT NULL,
    PRIMARY KEY (ID_SESSAO, ID_EVENTO),
    FOREIGN KEY (ID_SESSAO) REFERENCES SESSAO(ID),
    FOREIGN KEY (ID_EVENTO) REFERENCES EVENTO(ID)
);

CREATE TABLE SESSAO_SUBEVENTO (
    ID_SESSAO NUMBER NOT NULL,
    ID_SUBEVENTO NUMBER NOT NULL,
    PRIMARY KEY (ID_SESSAO, ID_SUBEVENTO),
    FOREIGN KEY (ID_SESSAO) REFERENCES SESSAO(ID),
    FOREIGN KEY (ID_SUBEVENTO) REFERENCES SUBEVENTO(ID)
);

CREATE TABLE EMAIL (
    ID_USUARIO NUMBER NOT NULL,
    EMAIL VARCHAR2(100) NOT NULL,
    PRIMARY KEY (ID_USUARIO, EMAIL),
    FOREIGN KEY (ID_USUARIO) REFERENCES USUARIO(ID)
);

CREATE TABLE PARTICIPACAO (
    ID_PARTICIPANTE NUMBER NOT NULL,
    ID_EVENTO NUMBER NOT NULL,
    PRIMARY KEY (ID_PARTICIPANTE, ID_EVENTO),
    FOREIGN KEY (ID_PARTICIPANTE) REFERENCES PARTICIPANTE(ID_USUARIO),
    FOREIGN KEY (ID_EVENTO) REFERENCES EVENTO(ID)
);

CREATE TABLE CERTIFICADO (
    ID NUMBER NOT NULL,
    ID_EVENTO NUMBER NOT NULL,
    ID_PARTICIPANTE NUMBER NOT NULL,
    PRIMARY KEY (ID),
    FOREIGN KEY (ID_EVENTO) REFERENCES EVENTO(ID),
    FOREIGN KEY (ID_PARTICIPANTE) REFERENCES PARTICIPANTE(ID_USUARIO)
);

CREATE TABLE EMITE (
    ID_EVENTO NUMBER NOT NULL,
    ID_PARTICIPANTE NUMBER NOT NULL,
    ID_CERTIFICADO NUMBER NOT NULL,
    PRIMARY KEY (ID_EVENTO, ID_PARTICIPANTE, ID_CERTIFICADO),
    FOREIGN KEY (ID_EVENTO) REFERENCES EVENTO(ID),
    FOREIGN KEY (ID_PARTICIPANTE) REFERENCES PARTICIPANTE(ID_USUARIO),
    FOREIGN KEY (ID_CERTIFICADO) REFERENCES CERTIFICADO(ID)
);

